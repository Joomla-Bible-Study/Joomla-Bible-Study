<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE install SYSTEM "http://dev.joomla.org/xml/1.5/component-install.dtd">
<install type="component"
         version="1.5.0"
         method="upgrade">
  <name>JBS Export Import</name>
  <version>1.5</version>
  <versionname>Beta4</versionname>
  <creationDate>2011-4-11</creationDate>
  <build>1</build>
  <author>JoomlaBibleStudy Team</author>
  <authorEmail>info@joomlabiblestudy.org</authorEmail>
  <authorUrl>http://www.JoomlaBibleStudy.org</authorUrl>
  <copyright>Joomla Bible Study</copyright>
  <license>GNU/GPL</license>
  <description>Joomla Bible Study Database Export/Import</description>
  <files>jbs_export_import.xml</files>
   <administration>
    <!-- Administration Menu Section -->
    <menu>JBS Export Import</menu>
    <!-- Administration Main File Copy Section -->
    <files>
      <filename>index.html</filename>
      <filename>admin.jbsexportimport.php</filename>
      <filename>controller.php</filename>
      <filename>restore.php</filename>
      <filename>backup.php</filename>
      <folder>views</folder>
      <folder>images</folder>
      <folder>help</folder>
      </files>
      <languages folder="languages">
      <language tag="en-GB">en-GB/en-GB.com_jbsexportimport.ini</language>
      </languages>
    </administration>
  <params />
</install>
